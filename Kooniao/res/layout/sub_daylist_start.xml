<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    style="@style/baseLinearLayoutMWHorizontalStyle"
    android:layout_marginTop="20dp" >

    <LinearLayout
        android:layout_width="60dp"
        android:layout_height="27dp"
        android:orientation="vertical" >

        <ImageView
            android:id="@id/tv_travel_day_end"
            android:layout_width="23dp"
            android:layout_height="23dp"
            android:layout_gravity="center_horizontal"
            android:contentDescription="@null"
            android:src="@drawable/travel_detail_day_end" />

        <View
            android:id="@id/v_vertical_top"
            android:layout_width="2dp"
            android:layout_height="5dp"
            android:layout_gravity="center|top"
            android:background="@color/vc7d3d7" />
    </LinearLayout>

    <TextView
        android:id="@id/tv_travel_detail_start_place"
        style="@style/baseLayoutWWStyle"
        android:layout_gravity="top"
        android:paddingTop="2dp"
        android:layout_marginLeft="5dp"
        android:background="@null"
        android:paddingLeft="5dp"
        android:textColor="@color/v020202"
        android:textSize="18sp" />

</LinearLayout>